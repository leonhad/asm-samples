all: FIRST.COM SECOND.COM ADD1.COM SUB1.COM MUL1.COM DIV1.COM SHIFT1.COM LOGICAL1.COM INC1.COM GUESS.COM SIEVE.COM

clean:
	del *.COM
	del *.LST

FIRST.COM: FIRST.ASM
	nasm -f bin FIRST.ASM -o FIRST.COM -l FIRST.LST

SECOND.COM: SECOND.ASM
	nasm -f bin SECOND.ASM -o SECOND.COM -l SECOND.LST

ADD1.COM: ADD1.ASM
	nasm -f bin ADD1.ASM -o ADD1.COM -l ADD1.LST

SUB1.COM: SUB1.ASM
	nasm -f bin SUB1.ASM -o SUB1.COM -l SUB1.LST

MUL1.COM: MUL1.ASM
	nasm -f bin MUL1.ASM -o MUL1.COM -l MUL1.LST

DIV1.COM: DIV1.ASM
	nasm -f bin DIV1.ASM -o DIV1.COM -l DIV1.LST

SHIFT1.COM: SHIFT1.ASM
	nasm -f bin SHIFT1.ASM -o SHIFT1.COM -l SHIFT1.LST

LOGICAL1.COM: LOGICAL1.ASM
	nasm -f bin LOGICAL1.ASM -o LOGICAL1.COM -l LOGICAL1.LST

INC1.COM: INC1.ASM
	nasm -f bin INC1.ASM -o INC1.COM -l INC1.LST

GUESS.COM: GUESS.ASM
	nasm -f bin GUESS.ASM -o GUESS.COM -l GUESS.LST

SIEVE.COM: SIEVE.ASM
	nasm -f bin SIEVE.ASM -o SIEVE.COM -l SIEVE.LST
